#define wiresize 64
#parties 2

typedef uint_t wiresize int
typedef uint_t wiresize*2 int2x

#input 1 int
#output 1 int
#input 2 int


function int modMul(int x, int y, int mod)
{
    int2x t0;
    int2x t1;

    t0{0:wiresize} = x;
    t1{0:wiresize} = y;

    int2x tmod;
    tmod{0:wiresize} = mod;

    return ((x**y)%tmod){0:wiresize};
}

function void main()
{
    int result = 1;

    /*base*/
    int a=input1;

    /*e*/
    int b=input2;

    int mod = input1 ^ input2;

    for(int i=0;i<wiresize;i++)
    {
        if(b{0}==1)
        {
            result=modMul(result,a,mod);
        }
        b = b>>1;
        a =modMul(a,a,mod);
    }

    output1 = result;
}
